{
    "builds": [
      {
        "src": "package.json",  
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"  
        }
      }
    ],
    "routes": [
      {
        "src": "/evento/:idUrl/:urlPersonalizada",
        "dest": "/index.html"  
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"  
      }
    ]
  }
  